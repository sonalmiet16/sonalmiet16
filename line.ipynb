    "from bokeh.plotting import figure, output_file, show\n",
    "\n",
    "x = [10,20,30,40]\n",
    "y = [2,4,6,8]\n",
    "output_file(\"line.html\")\n",
    "\n",
    "p= figure(title=\"basic line plot\", x_axis_label=\"x-axis\", y_axis_label=\"y-axis\")\n",
    "\n",
    "p.line(x, y, legend='price',line_width=3)\n",
    "\n",
    "show(p)\n"
